{
  "name": "admin-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon --exec tsx server.ts --watch server.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.ts --outDir dist/server",
    "preview": "cross-env NODE_ENV=production nodemon --exec tsx server.ts"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "glob": "^10.3.10",
    "happy-dom": "^12.10.3",
    "lit": "^3.1.0",
    "nanostores": "^0.9.5",
    "nodemon": "^3.0.1",
    "sirv": "^2.0.3",
    "tsx": "^4.4.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.9.0",
    "cross-env": "^7.0.3",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  }
}
